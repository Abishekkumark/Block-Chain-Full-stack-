let num1=10
let num2=20
let num3=30
// Addition of n1 and n2
let add=num1+num2
//Subraction of n1 and n3
let sub=num1-num3
//multiply of n2 and n3
let mul=num2*num3
//division of n1 and n3
let div=num1/num3

console.log("value of addition is "+add)
console.log("value of subraction is "+sub)
console.log("value of multiplication is "+mul)
console.log("value of division is "+div)
